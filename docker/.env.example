# Tailscale auth key (must be reusable for container restarts)
# Generate at: https://login.tailscale.com/admin/settings/keys
TS_AUTHKEY=tskey-auth-XXXXX

# Omni domain (your Tailscale MagicDNS hostname)
# Format: omni.<tailnet>.ts.net
OMNI_DOMAIN=omni.your-tailnet.ts.net

# Initial admin user (must match exact email from OIDC)
OMNI_INITIAL_USER=you@example.com

# OIDC configuration from tsidp
# Issuer URL is your tsidp's Tailscale hostname
OIDC_ISSUER_URL=https://tsidp.your-tailnet.ts.net

# Client credentials from tsidp admin UI or dynamic registration
OIDC_CLIENT_ID=
OIDC_CLIENT_SECRET=

# Omni Infrastructure Provider key (for Proxmox provider)
# Generate in Omni UI: Settings > Infrastructure Providers > Create Provider
OMNI_INFRA_PROVIDER_KEY=
